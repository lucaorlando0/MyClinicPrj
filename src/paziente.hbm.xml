<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
 <class name="Paziente" table="paziente">
  <id name="CF" column="CF">
  <generator class="increment"></generator>
  </id>
  <property column="nome" generated="never" lazy="false" name="nome"/>
  <property column="cognome" generated="never" lazy="false" name="cognome"/>
  <property column="email" generated="never" lazy="false" name="email"/>
  <property column="cellulare" generated="never"  lazy="false" name="cellulare"/>
  <property column="operazioni_passate" generated="never"  lazy="false" name="operazioni_passate"/>
 </class>
</hibernate-mapping>